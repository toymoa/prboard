{
       "require": {
        "php": "^8.4",
        "ext-redis": "*",
        "slim/slim": "^4.12",
        "slim/psr7": "^1.6",
        "twig/twig": "^3.7",
        "slim/twig-view": "^3.3",
        "monolog/monolog": "^3.4",
        "vlucas/phpdotenv": "^5.5",
        "symfony/serializer": "^7.1",
        "symfony/property-access": "^7.1"
    },redisboard/app",
    "description": "Redis-based PHP bulletin board using Slim 4",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "slim/slim": "^4.12",
        "slim/psr7": "^1.6",
        "predis/predis": "^2.2",
        "twig/twig": "^3.7",
        "slim/twig-view": "^3.3",
        "monolog/monolog": "^3.4",
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-strict-rules": "^1.5",
        "phpstan/phpstan-phpunit": "^1.3",
        "squizlabs/php_codesniffer": "^3.7",
        "friendsofphp/php-cs-fixer": "^3.35",
        "psalm/plugin-phpunit": "^0.18",
        "vimeo/psalm": "^5.15"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse",
        "phpcs": "phpcs",
        "phpcbf": "phpcbf", 
        "psalm": "psalm",
        "cs-fix": "php-cs-fixer fix",
        "qa": [
            "@phpstan",
            "@phpcs",
            "@psalm",
            "@test"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
